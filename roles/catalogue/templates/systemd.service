[Unit]
Description = Catalogue Service
[Service]
User=roboshop
Environment=DOCUMENTDB=true
Environment=MONGO_URL="mongodb://{{SECRETS.DOCDB_USERNAME}}:{{SECRETS.DOCDB_PASSWORD}}@mongodb-{{ENV}}.roboshop.internal:27017/catalogue?tls=true&replicaSet=rs0&readPreference=secondaryPreferred&retryWrites=false" 
ExecStart=/bin/node /home/roboshop/catalogue/server.js
SyslogIdentifier=catalogue

[Install]
WantedBy=multi-user.target